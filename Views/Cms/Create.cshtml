            function UploadFileToServer() {

                var uploadActiveVal = $("#uploadActive").val();
                var fileNameVal = $("#fileName").val();

                if (uploadActiveVal != "0" && fileNameVal != "") {

                    $.ajax({
                        url: '/Cms/UploadFileToServer',
                        data: { clientName: fileNameVal },
                        type: 'post',
                        //  contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (output) {
                            $("#uploadActive").val("0");
                            $("#fileName").val("");
                            if (output.status == "0")
                                alert("File is not uploaded. Something went wrong");
                            else
                                alert("File Uploaded SuccessFully");
                        },
                        error: function (output) {

                            alert("Problem in Uploading , Go to Edit section to upload the file ");

                        }
                    });
                }
                else {
                    alert("Upload is not active");
                }

                return false;


            }

