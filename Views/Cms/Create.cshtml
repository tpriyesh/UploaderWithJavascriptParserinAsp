                    Color20: color20,
                    Color21: color21,
                    Color22: color22,
                    Color23: color23,
                    Color24: color24,
                    Color25: color25,
                    Color26: color26,
                    MenuImageUrl: menuImageUrl,
                    AdminParams: adminParams,
                    NextArrowUrl: nextArrowUrl,
                    PrevArrowUrl: prevArrowUrl,


                    isAfp: isAfp,
                    Component: objComponent,
                    Series: objSeriesArray
                }

                $.ajax({
                    url: '/Cms/Save',
                    data: JSON.stringify(obj),
                    type: 'post',
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (output) {
                        EmptyValues();
                        MakeUploadActive(output);
                        $("#clientNameCheck").val("0");
                        alert("SuccessFully Saved . Now Upload is Active .You can click Upload to Upload File Else you can go to edit section to upload file");
                    },
                    error: function (output) {
                        EmptyValues();
                        alert("Something went wrong");
                    }
                });

                return false;

            }


            function CheckClientNameAvailability() {

                var clientName = $("#ClientName").val();
                if (clientName != "") {

                    $.ajax({
                        url: '/Cms/CheckClientNameAvailability',
                        data: { clientName: clientName },
                        type: 'post',
                        //contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (output) {
                            if (output.data == true) {
                                $("#clientNameCheck").val("1");
                                // alert("Client already exists . Please Enter Different Name");
                                return false;

                            }
                            else {
                                $("#clientNameCheck").val("0");
                                return true;

                            }
                        }
                    });


                }




            }

            function SuccessCallback(result) {


                $.each(result.component, function () {


                    if (this.ComponentName == "PlayerStats") {
                        if (this.IsChecked == true) {
                            $("#PlayerStats").prop("checked", true);
                        }
                    }

                    if (this.ComponentName == "Standings") {
                        if (this.IsChecked == true) {
                            $("#Standings").prop("checked", true);
                        }
                    }
                    if (this.ComponentName == "News") {
                        if (this.IsChecked == true) {
                            $("#News").prop("checked", true);
                        }
                    }
                    if (this.ComponentName == "FormGuide") {
                        if (this.IsChecked == true) {
                            $("#FormGuide").prop("checked", true);
                        }
                    }
                    if (this.ComponentName == "LineUps") {
                        if (this.IsChecked == true) {
                            $("#LineUps").prop("checked", true);
                        }
                    }
                    if (this.ComponentName == "Commentary") {
                        if (this.IsChecked == true) {
                            $("#Commentary").prop("checked", true);
                        }
                    }
                    if (this.ComponentName == "Scorecard") {
                        if (this.IsChecked == true) {
                            $("#Scorecard").prop("checked", "checked");
                        }
                    }
                    if (this.ComponentName == "MatchStats") {
                        if (this.IsChecked == true) {
                            $("#MatchStats").prop("checked", true);
                        }
                    }



                });
                $.each(result.series, function () {

                    var self = this;
                    $('.series').each(function (index, value) {

                        var id = $(this).attr("id");

                        if (self.Id == id) {
                            $(this).prop("checked", true);


                        }






                    });

                });




                //   $("#matchTabsForegroundColor").val(result.matchTabsForegroundColor);
                // console.log(result);


            }

            function LoadClientInformation() {
                var clientName = $("#Clients option:selected").text();



                $.ajax({
                    url: '/Cms/GetClientInformation',
                    data: { clientName: clientName },
                    type: 'post',
                    // contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: function (result) {
                        SuccessCallback(result);
                    }
                });



            }


            $("#btnSubmit").click(InsertRecord);
            $("#Clients").change(LoadClientInformation)

            $("#ClientName").keyup(CheckClientNameAvailability);
            jQuery('.numbersOnly').keyup(function () {
                this.value = this.value.replace(/[^0-9\.]/g, '');
            });



            $("#btnUpload").click(UploadFileToServer);




        });

    </script>



